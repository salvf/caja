  <script src="/javascripts/socket.io.min.js"></script>

  <script>
    var socket = io();
    var turnos = [];
    var aux = [];
    socket.on('json', function(msg) {
      console.log(msg)
      turnos.push(msg)
      if(turnos.length > 4){
        turnos.reverse().pop();
        aux = turnos;
        turnos.reverse();
      }else{
        aux = turnos.reverse();
        turnos.reverse();
      }
      aux.reverse()
      for(var x=0;x<aux.length;x++){
        var idpedido= "#pedido".concat(x+1)
        var idfila= "#fila".concat(x+1)
        var idnombre= "#nombre".concat(x+1)
        $(idpedido).text("Pedido "+aux[x].pedido)
        $(idfila).text("Fila "+aux[x].fila)
        $(idnombre).text("Atiende   "+aux[x].cliente)
        //$(idnombre).html("Atiende   <center> "+aux[x].cliente+"</center>")
      }
      aux.reverse()
      console.log(aux)
    });
  </script>

